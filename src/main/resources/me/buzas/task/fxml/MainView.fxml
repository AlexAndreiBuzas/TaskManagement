<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.layout.VBox?>
<SplitPane dividerPositions="0.25"
           prefHeight="400.0" prefWidth="716.0"
           xmlns:fx="http://javafx.com/fxml/1"
           fx:controller="me.buzas.task.ui.controller.MainController">

    <VBox alignment="CENTER" spacing="16.0">
        <Button onAction="#goToProjects" prefHeight="40.0" prefWidth="Infinity" text="Projects"/>
        <Button onAction="#goToTasks" prefHeight="40.0" prefWidth="Infinity" text="Tasks"/>
        <Button onAction="#goToUsers" prefHeight="40.0" prefWidth="Infinity" text="Users"/>
        <Button onAction="#goToReports" prefHeight="40.0" prefWidth="Infinity" text="Report"/>
        <padding>
            <Insets bottom="16.0" left="16.0" right="16.0" top="16.0"/>
        </padding>
    </VBox>

    <Pane fx:id="contentArea" VBox.vgrow="ALWAYS"/>
    <padding>
        <Insets bottom="8.0" left="8.0" right="8.0" top="8.0"/>
    </padding>
</SplitPane>
