<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<AnchorPane prefHeight="184.0" prefWidth="250.0" xmlns="http://javafx.com/javafx/17.0.12"
            xmlns:fx="http://javafx.com/fxml/1" fx:controller="me.buzas.task.ui.controller.CreateUserPopupController">
    <children>
        <VBox layoutX="13.0" layoutY="14.0" prefHeight="80.0" prefWidth="225.0" spacing="8.0">
            <children>
                <TextField fx:id="usernameField" promptText="Username"/>
                <TextField fx:id="passwordField" promptText="Password"/>
                <TextField fx:id="teamField" promptText="Team"/>
                <TextField fx:id="positionInsideTeamField" prefHeight="26.0" prefWidth="149.0" promptText="Position"/>
            </children>
        </VBox>
        <HBox alignment="CENTER" layoutX="22.0" layoutY="157.0" prefHeight="26.0" prefWidth="200.0" spacing="32.0">
            <children>
                <Button onAction="#handleCancel" text="Cancel"/>
                <Button onAction="#handleCreateUser" text="Create User"/>
            </children>
        </HBox>
    </children>
    <padding>
        <Insets bottom="8.0" left="8.0" right="8.0" top="8.0"/>
    </padding>
</AnchorPane>
